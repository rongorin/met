@model MetopeMVCApp.Models.AddSecurityListDetailVM

@{
    ViewBag.Title = "Create";
}

<h3>Create a Securities Listing</h3>

@using (Html.BeginForm()) 
{ 
    @Html.AntiForgeryToken()
    <br>
    <div class="form-horizontal">  
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.Entity_ID)

         
        @Html.EditorFor(model => model.Security_List_Code, "BootstrapEditorVertic")
         
        @Html.EditorForModel("Security_List_Detail")
    </div>
}

@section Scripts {
    @Scripts.Render("~/bundles/datetime")

}