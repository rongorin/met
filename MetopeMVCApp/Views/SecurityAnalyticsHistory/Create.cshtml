@model Metope.DAL.Security_Analytics_History

<h2>Create new Security Analytics History  </h2>
@section Styles {
    @Styles.Render("~/Content/datetime")
}
 
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <hr />
        @Html.ValidationSummary(false)
        @Html.ValidationMessage("Error")
        <input type="hidden" id="navIndicator" name="navIndicator" value=@ViewBag.Nav /> 

         <div class="form-group">   
            <div>
                @Html.EditorFor(model => model.Record_Date, "BootstrapEditDateVertic")
            </div> 
        </div>

        @Html.EditorForModel()

    </div>
} 
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")  
    @Scripts.Render("~/bundles/datetime")
}

