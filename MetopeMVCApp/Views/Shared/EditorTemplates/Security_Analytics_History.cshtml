<div class="col-sm-4">
    <div class="form-group">
        @Html.LabelFor(model => model.Security_ID, htmlAttributes: new { @class = "control-label col-md-5 input-sm" })
        <div class="col-md-7">
            @if (ViewBag.MeDisabled == "readonly")
            {
                @Html.DropDownList("Security_ID", (SelectList)ViewBag.Securities_All, string.Empty, new
               {
                   @class = "form-control",
                   @readonly = "readonly",
                   disabled = "disabled"
               })
            }
            else
            {
                @Html.DropDownList("Security_ID", (SelectList)ViewBag.Securities_All, string.Empty, new { @class = "form-control" })
            }

            @Html.ValidationMessageFor(model => model.Security_ID, "", new { @class = "text-danger" })
        </div>
    </div>
</div> 

