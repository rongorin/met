@model MetopeMVCApp.Models.Security_Detail

@{
    ViewBag.Title = "Edit";
}
 <h4></h4>
<div class="panel panel-primary">
    <div class="panel-heading panel-head">Editing : Security <b>  @Model.Short_Name </b></div>
    <div class="panel-body">
        @using (Html.BeginForm(null, null, FormMethod.Post, new { @class = "form-horizontal" }))
        {
            @Html.AntiForgeryToken()

            <div class="form-horizontal">

                @Html.ValidationSummary(true)
                @Html.HiddenFor(model => model.Security_ID)
                @Html.HiddenFor(model => model.Entity_ID)

                @Html.EditorForModel(new { PreviousYearCount = 10, NextYearCount = 10 }) 
            </div>
        }
    </div>
</div>
<div>
    @Html.ActionLink("Back to List", "Index")
</div>
<script type="text/javascript">
    $(document).on('click', '#backLink', function () {
        alert("ss");
        history.go(-1);
    });
</script>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
